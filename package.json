{
    "name": "partnerdo-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@chakra-ui/icons": "^1.0.3",
        "@chakra-ui/react": "^1.1.6",
        "@emotion/react": "^11.1.4",
        "@emotion/styled": "^11.0.0",
        "@hookform/resolvers": "^1.3.2",
        "@reduxjs/toolkit": "^1.4.0",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.3",
        "@testing-library/user-event": "^12.6.2",
        "@types/react-router-dom": "^5.1.7",
        "axios": "^0.21.1",
        "framer-motion": "^3.2.2-rc.1",
        "lodash": "^4.17.20",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-hook-form": "^6.14.2",
        "react-paginate": "^7.1.0",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.1",
        "react-use": "^17.1.1",
        "typescript": "~4.1.3",
        "yup": "^0.32.8"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@storybook/addon-actions": "^6.1.15",
        "@storybook/addon-essentials": "^6.1.15",
        "@storybook/addon-links": "^6.1.15",
        "@storybook/node-logger": "^6.1.15",
        "@storybook/preset-create-react-app": "^3.1.5",
        "@storybook/react": "^6.1.15",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.22",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-paginate": "^6.2.1",
        "@types/react-redux": "^7.1.16",
        "@types/webpack-env": "^1.15.3",
        "@types/yup": "^0.29.11",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "husky": "^4.3.8",
        "postcss-cli": "^8.3.1",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.0.2",
        "react-app-rewired": "^2.1.8"
    },
    "scripts": {
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "build:prod": "yarn lint && yarn test --watchAll=false && yarn build",
        "eject": "react-scripts eject",
        "format": "pretty-quick --staged",
        "lint": "eslint src/ --ext .ts,.tsx,.js",
        "test": "react-app-rewired test",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "moduleNameMapper": {
            "@assets/(.*)$": "<rootDir>/src/assets/$1",
            "@components/(.*)$": "<rootDir>/src/components/$1",
            "@consts/(.*)$": "<rootDir>/src/consts/$1",
            "@hooks/(.*)$": "<rootDir>/src/hooks/$1",
            "@layouts/(.*)$": "<rootDir>/src/layouts/$1",
            "@models/(.*)$": "<rootDir>/src/models/$1",
            "@routing/(.*)$": "<rootDir>/src/routing/$1",
            "@utils/(.*)$": "<rootDir>/src/utils/$1",
            "@slices/(.*)$": "<rootDir>/src/slices/$1",
            "@store/(.*)$": "<rootDir>/src/store/$1",
            "@screens/(.*)$": "<rootDir>/src/screens/$1",
            "@services/(.*)$": "<rootDir>/src/services/$1",
            "@src/(.*)$": "<rootDir>/src/$1",
            "@theme/(.*)$": "<rootDir>/src/theme/$1"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn format && yarn lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
